:query-file :is <./query-action.rq.n3>.

{?query-file :query-result ?z} <= {
    ?query-file log:semantics ?sem.
    ?sem log:includes {?y a :Query; :select ?s; :where ?w}.
    # "T11" log:trace ( ?s ?w ) .
    ?scope e:findall (?s ?w ?z). 
} .

{ :query-file :is ?file. ?file :query-result ?z } => {:answer :is ?z}.

